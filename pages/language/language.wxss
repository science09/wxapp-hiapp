.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
}

.cell {
  height: 60rpx;
  padding: 20rpx 40rpx;
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid #d9d9d9;
}

.cell-bottom {
  height: 60rpx;
  padding: 20rpx 40rpx;
  display: flex;
  align-items: center;
}